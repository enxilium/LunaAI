{
  "name": "luna-ai",
  "productName": "luna-ai",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/main/main.js",
  "scripts": {
    "dev": "concurrently \"webpack serve --config webpack.dev.js\" \"cross-env NODE_ENV=development electron . --trace-warnings\"",
    "package": "electron-forge package",
    "make": "webpack --config webpack.prod.js && electron-forge make",
    "publish": "webpack --config webpack.prod.js && electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "enxilium",
    "email": "79335627+enxilium@users.noreply.github.com"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/preset-typescript": "^7.27.0",
    "@langchain/core": "^0.3.58",
    "@langchain/google-genai": "^0.2.12",
    "@material/web": "^2.3.0",
    "@picovoice/porcupine-react": "^3.0.3",
    "@picovoice/porcupine-web": "^3.0.3",
    "@picovoice/web-voice-processor": "^4.0.9",
    "dotenv": "^16.5.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.1.0",
    "keytar": "^7.9.0",
    "langchain": "^0.3.28",
    "mic": "^2.1.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "node-microphone": "^0.1.6",
    "node-wit": "^6.6.0",
    "open": "^10.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "styled-components": "^6.1.17",
    "uuid": "^11.1.0",
    "wav": "^1.0.2",
    "whisper.cpp": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "electron": "35.2.0",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^5.10.0"
  },
  "build": {
    "appId": "com.luna.assistant",
    "productName": "Luna AI Assistant",
    "files": [
      "**/*",
      "!assets/**/*"
    ],
    "extraResources": [
      {
        "from": "assets",
        "to": "assets"
      }
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
